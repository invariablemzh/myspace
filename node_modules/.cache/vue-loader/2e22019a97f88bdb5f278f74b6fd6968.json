oggler\" type=\"button\" data-bs-toggle=\"collapse\" data-bs-target=\"#navbarText\" aria-controls=\"navbarText\" aria-expanded=\"false\" aria-label=\"Toggle navigation\">\n      <span class=\"navbar-toggler-icon\"></span>\n    </button>\n    <div class=\"collapse navbar-collapse\" id=\"navbarText\">\n      <ul class=\"navbar-nav me-auto mb-2 mb-lg-0\">\n        <li class=\"nav-item\">\n          <router-link class=\"nav-link \" :to=\"{name: 'home'}\">首页</router-link>\n        </li>\n        <li class=\"nav-item\">\n          <router-link class=\"nav-link\" :to=\"{name: 'userlist'}\">好友列表</router-link>\n        </li>\n      </ul>\n      <ul class=\"navbar-nav\" v-if=\"!$store.state.user.is_login\">\n        <li class=\"nav-item\">\n          <router-link class=\"nav-link \" :to=\"{name: 'login'}\">登录</router-link>\n        </li>\n        <li class=\"nav-item\">\n          <router-link class=\"nav-link\" :to=\"{name: 'register'}\">注册</router-link>\n        </li>\n      </ul>\n      <ul class=\"navbar-nav\" v-else>\n        <li class=\"nav-item\">\n          <router-link class=\"nav-link \" :to=\"{name: 'userprofile', params: {userId: $store.state.user.username }}\"> {{$store.state.user.username}} </router-link>\n        </li>\n        <li class=\"nav-item\">\n          <a class=\"nav-link\" style=\"cursor: pointer\" @click=\"logout\">退出</a>\n        </li>\n      </ul>\n\n\n    </div>\n  </div>\n</nav>\n</template>\n\n\n\n<script>\nimport { useStore } from 'vuex';\n\nexport default {\n    name: \"NavBar\",\n    setup() {\n      const store = useStore();\n      const logout = () => {\n        store.commit('logout');\n      };\n\n      return{\n        logout,\n      }\n    }\n}\n</script>\n\n\n<style scoped>\n</style>\n"]}]}